{
  "name": "mern-auth-ts",
  "version": "1.0.0",
  "description": "Basic MERN Authentication Setup",
  "main": "index.js",
  "scripts": {
    "full-install": "npm i && npm i --prefix client && npm i --prefix server",
    "client-install": "npm i --prefix client",
    "server-install": "npm i --prefix server",
    "start": "npm run full-install && npm run dev",
    "server": "npm run --prefix server server-watch",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run --prefix server server-watch\" \"npm start --prefix client\"",
    "test": "npm test"
  },
  "keywords": [
    "mongodb",
    "expressjs",
    "nodejs",
    "reactjs"
  ],
  "author": "Jacob Ashcraft",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.1.0"
  }
}
